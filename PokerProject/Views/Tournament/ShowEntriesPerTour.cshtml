@model IEnumerable<PokerProject.ViewModels.ShowTourEntries>
@{
    ViewBag.Title = "ShowEntriesPerTour";
}

    <table class="table table-bordered">

        <tr>
            <th>TournamentId</th>
            <th>Number of players</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelitem => item.TournamentId)</td>
                <td>@item.PlayersCount</td>
            </tr>
        }

    </table>

